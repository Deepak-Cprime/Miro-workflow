{
  "name": "miro-workflow-analyzer",
  "version": "1.0.0",
  "description": "AI-powered workflow analysis for Miro boards using OpenAI GPT-4",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
  "build": "tsc && if not exist dist/server.js (echo Build failed: dist/server.js not found && exit 1)",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "analyze": "npm run dev",
    "server": "tsx src/server.ts",
    "start:server": "node dist/server.js"
  },
  "keywords": [
    "miro",
    "workflow",
    "analysis",
    "openai",
    "ai",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "openai": "^5.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^22.5.4",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
